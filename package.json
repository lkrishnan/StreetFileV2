		"vue-router": "^3.2.0",
		"vuelidate": "^0.7.6",
		"vuetify": "^2.4.0",
		"vuex": "^3.4.0"
  	},
  	"devDependencies": {
		"@vue/cli-plugin-babel": "~4.5.0",
